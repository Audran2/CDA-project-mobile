{
  "extends": "expo/tsconfig.base",
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": true,
  "compilerOptions": {
    "jsx": "react-native",
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "target": "ES6",
    "typeRoots": [
      "./types"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.jsx"
  ]
}
